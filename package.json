{
  "name": "builder-mvp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "build:prod": "NODE_ENV=production next build",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "docker:build": "docker build -f Dockerfile.prod -t vcm-dashboard .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.production vcm-dashboard",
    "deploy": "npm run build:prod && docker-compose -f docker-compose.prod.yml up --build -d",
    "deploy:stop": "docker-compose -f docker-compose.prod.yml down",
    "deploy:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "deploy:status": "docker-compose -f docker-compose.prod.yml ps",
    "health": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "backup": "./scripts/backup.sh",
    "clean": "docker system prune -f && docker image prune -f"
  },
  "dependencies": {
    "@fal-ai/serverless-client": "^0.15.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@huggingface/inference": "^4.13.5",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.447.0",
    "marked": "^17.0.1",
    "next": "14.2.33",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "openai": "^6.16.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.66.0",
    "recharts": "^3.4.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
